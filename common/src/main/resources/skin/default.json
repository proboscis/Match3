{
        com.badlogic.gdx.graphics.g2d.BitmapFont:{
            yggdrasil-font:{file:font/yggdrasil.fnt},
            default-font:{file:font/Harrington.fnt}
        },
        com.badlogic.gdx.graphics.Color: {
            white: { r: 1, g: 1, b: 1, a: 1 },
            gray:{r:0.6,g:0.6,b:0.6,a:1},
            dark_gray:{r:0.2,g:0.2,b:0.2,a:1},
            red:{r:1,g:0,b:0,a:1}
        },
        com.badlogic.gdx.scenes.scene2d.ui.Skin$TintedDrawable: {
                	gray_back: { name: data/dummy, color:gray},
                	red_back:{name:data/dummy,color:red},
                	dark_gray_back:{name:data/dummy,color:dark_gray}
                },
        com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle:{
            default:{
                font:default-font,
                fontColor:white
            }
        },
        com.badlogic.gdx.scenes.scene2d.ui.List$ListStyle: {
        	default: {
        	fontColorUnselected: gray,
        	 selection:gray_back,
        	  fontColorSelected: white,
        	   font: default-font
        	    }
        },
        com.badlogic.gdx.scenes.scene2d.ui.ScrollPane$ScrollPaneStyle: {
        	default: {
        	 vScroll: dark_gray_back,
        	  hScrollKnob: gray_back,
        	   background: dark_gray_back,
        	    hScroll: dark_gray_back,
        	     vScrollKnob: gray_back
        	      }
        },
        com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
            default: { 
                down: dark_gray_back,
                up: gray_back,
                font: default-font, 
                fontColor: white
            },
            toggle: {
                down: dark_gray_back, 
                up: gray_back, 
                checked: gray_back, 
                font: default-font, 
                fontColor: white, 
                downFontColor: red
            }
        }
}